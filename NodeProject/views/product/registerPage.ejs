    <!-- Navbar -->
    <% include ./../tips/subs/navbar %>
    <!-- </header> -->
    <nav class="before-breadcrumb" aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item fs-20"><a href="./">Home</a></li>
            <!-- <li class="breadcrumb-item"><a href="#">Library</a></li> -->
            <li class="breadcrumb-item fs-20 active" aria-current="page">Login & Register</li>
        </ol>
    </nav>
    <section class="seperator-wrapper">
        <div class="seperator gradient">
        </div>
    </section>
    <div class="main-container">
        <div class="row">
            <div class="col-md-5 login-form">
                <div class="title">
                    <h2 class="my-3">Login</h2>
                </div>
                <div class="authentication-button px-6">
                    <a class="facebook-button btn-outline" href="#">
                        <div class="btn-facebook-cover text-center">
                                <span class="name">Login with Facebook</span>
                        </div>
                    </a>
                    <br>
                    <a class="google-button" href="#">
                        <div class="btn-google-cover text-center">
                            <span class="name">Login with Google</span>
                        </div>
                    </a>
                </div>
                <div class="separator px-6">Or</div>
                <form id="form-login" class="form-not-mg px-6" autocomplete="on" method="post">
                    <div class="form-group">
                        <label for="email">Email address</label>
                        <input autocomplete="on" type="email" name="email" class="form-control" id="email" value="" aria-describedby="emailHelp" placeholder="Email">
                        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
                    </div>
                    <div class="form-group">
                        <label for="password">Password</label>
                        <input autocomplete="on" type="password" name="password" class="form-control" id="password" value="" placeholder="Password">
                    </div>
                   
                    <button type="submit" id="btn-login" class="btn btn-primary btn-login">Login</button>

                </form>
            </div>
            <div class="col-md-6 reg-form">
                <div class="title">
                    <h2 class="my-3">Register</h2>
                </div>
                <form class="form-not-mg" id='form-register' method='POST' >
                    <div class='row'>
                        <div class='col-md-12'>
                            <div class='col-md-6 form-group'>
                                <label for='fname'>First name</label>
                                <input required type='text' name='fname' class='form-control' id='fnam' aria-describedbsy='fnamelHelp' placeholder='First name'>
                            </div>
                            <div class='col-md-6 form-group'>
                                <label for='lname'>Last name</label>
                                <input required type='text' name='lname' class='form-control' id='lname' aria-describedbsy='lnameHelp' placeholder='Last name'>
                            </div>
                        </div>
                    </div>
                    <div class='row'>
                        <div class='col-md-12'>
                            <div class='form-group col-md-12'>
                                <label for='reg_email'>Email</label>
                                <input required type='email' name='email' class='form-control' id='reg_email' placeholder='Email'>
                                <small id='Help' class='form-text text-muted'>
                                    Ideally, you should register your email with paypal email
                                </small>
                            </div>
                            <div class='form-group col-md-12'>
                                <label for='reg_password'>Password</label>
                                <input required type='password' name='password' class='form-control' id='reg_password' placeholder='Password'>
                            </div>
                            <div class='form-group col-md-12'>
                                <label for='re_password'>Confirm Password</label>
                                <input required type='password' name='re_password' class='form-control' id='re_password' placeholder='Re-Password'>
                            </div>
                        </div>
                    </div>
                    <div class='row'>
                        <div class='form-group col-md-12'>
                            <div class='col-md-12 g-recaptcha' data-sitekey='6Lcti9UUAAAAADIUivaJtXit0OKia78-EQIbnDkc'>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <button type='submit' id='btn-reg' class='btn btn-primary btn-register'>Register</button>
                    </div>
                </form>
            </div>
            
        </div>
    </div>
    <footer>
            <div class="col-full ss-copyright">
