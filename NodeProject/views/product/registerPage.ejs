  <script src="/api/js/pjs/main.js"></script>
  <script src="/share/google-capcha/api.js"></script>
  <script src="/share/sweetalert/sweetalert2@9.js"></script>
  <script src="/api/js/pjs/product/registerPage/js.js"></script>
  <script src="https://apis.google.com/js/platform.js" async defer></script>
