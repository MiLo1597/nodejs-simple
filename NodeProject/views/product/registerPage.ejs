<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Sign Up</title>
    <!-- Meta -->
    <% include ./../tips/subs/meta %>
    <!-- Loading effect -->
    <link
      rel="stylesheet"
      type="text/css"
      href="/api/css/effect/preload/preload-signal.css"
    />
    <!-- Library -->
    <link
      rel="stylesheet"
      type="text/css"
      href="/api/css/pcss/line-awesome-1.3.0/css/line-awesome.min.css"
    />
    <link rel="stylesheet" href="/share/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="/package/waitMe/waitMe.min.css" />
    <!-- Base css -->
    <link rel="stylesheet" type="text/css" href="/api/css/base/font.css" />
    <link rel="stylesheet" type="text/css" href="/api/css/base/main.css" />
    <link rel="stylesheet" type="text/css" href="/api/css/base/footer.css" />
    <link
      rel="stylesheet"
      type="text/css"
      href="/api/css/base/breadcrumb.css"
    />
    <link rel="stylesheet" type="text/css" href="/api/css/base/navbar.css" />
    <!-- Customize CSS -->
    <link
      rel="stylesheet"
      type="text/css"
      href="/api/css/effect/rgb-wrapper/style.css"
    />
    <link rel="stylesheet" href="/api/css/effect/typing-text.css" />
    <link
      rel="stylesheet"
      type="text/css"
      href="/api/css/pcss/registerPage/style.css"
    />
    <!--=== script ===-->
    <script src="/api/js/pjs/modernizr.js"></script>

    <!--=== favicons ===-->
    <link rel="shortcut icon" href="/favicon/favicon.ico" type="image/x-icon" />
    <link rel="icon" href="/favicon/favicon.ico" type="image/x-icon" />
  </head>
  <style>
    .abcRioButtonLightBlue {
      width: 100% !important;
    }
  </style>

  <body>
    <% include ./../tips/effect/preload-signal %> <% include
    ./../tips/subs/sample-navbar %> <% include
    ./../tips/subs/sample-breadcrumb%>
    <div class="main-container">
      <div class="row">
        <div
          class="d-flex col-md-12 g-3 justify-content-between"
          style="min-height: 650px; max-height: 100%"
        >
          <div class="col-md-7 col-xs-12 px-4 container-banner">
            <div class="login-banner"></div>
          </div>
          <div class="col-md-5 col-xs-12 px-4 container-form">
            <div class="rainbow">
              <span></span>
              <span></span>
            </div>
            <div id="form-group-container">
              <div class="d-flex w-100 tabs-customize">
                <span id="tab-sign-in" class="w-100 text-center active-tab mx-4"
                  ><a href="#" class="text-decoration-none"
                    >Existing user</a
                  ></span
                >
                <span
                  class="w-1"
                  style="border: 1px solid var(--title-color)"
                ></span>
                <span id="tab-reg" class="w-100 text-center mx-4"
                  ><a href="#" class="text-decoration-none">New user</a></span
                >
              </div>
              <!-- login form -->
              <div class="login-form">
                <div class="d-flex justify-content-center title my-3">
                  <h2 class="effect-typing-text-sign-in">Sign in</h2>
                </div>
                <!-- <div class="authentication-button">
                  <a class="facebook-button btn-outline" href="#">
                    <div class="btn-facebook-cover text-center">
                      <span class="name">Login with Facebook</span>
                    </div>
                  </a>
                  <br />
                  <a class="google-button" href="#">
                    <div class="btn-google-cover text-center">
                      <div class="g-signin2" data-onsuccess="onSignIn"></div>
                    </div>
                  </a>
                </div>
                <div class="separator">Or</div> -->
                <form
                  id="form-login"
                  class="form-not-mg"
                  autocomplete="on"
                  method="post"
                >
                  <div class="form-group">
                    <label for="email">Email</label>
                    <div class="input-group mb-3 align-items-end">
                      <span class="input-group-text" id="emailHelp"
                        ><i class="las la-envelope la-2x"></i
                      ></span>
                      <input
                        autocomplete="on"
                        type="email"
                        name="email"
                        class="form-control"
                        id="email"
                        value=""
                        aria-describedby="emailHelp"
                        placeholder="Email"
                        aria-label="Email"
                        required
                      />
                    </div>
                    <small id="emailHelp" class="form-text text-muted"
                      >We'll never share your email with anyone else.</small
                    >
                  </div>
                  <div class="form-group">
                    <label for="password">Password</label>
                    <div class="input-group mb-3 align-items-end">
                      <span class="input-group-text" id="pwHelp"
                        ><i class="las la-lock la-2x"></i
                      ></span>
                      <input
                        autocomplete="on"
                        type="password"
                        name="password"
                        class="form-control"
                        id="password"
                        placeholder="Password"
                        aria-describedby="pwHelp"
                        aria-label="Password"
                        required
                      />
                      <span
                        id="sign_eye-pw__show"
                        class="input-group-text"
                        style="cursor: pointer"
                        data-visible="visible"
                        ><i class="las la-eye-slash la-2x"></i
                      ></span>
                      <span
                        id="sign_eye-pw__hide"
                        class="input-group-text hide-pw"
                        style="cursor: pointer"
                        data-visible="hidden"
                        ><i class="las la-eye la-2x"></i
                      ></span>
                    </div>
                    <div class="forgot-pw position-relative">
                      <a href="/reset-password">Forgot password?</a>
                    </div>
                  </div>
                  <div class="btn-area">
                    <button type="submit" id="btn-sign-in" class="btn-login">
                      Login
                    </button>
                  </div>
                </form>
              </div>
              <!-- reg-form -->
              <div class="reg-form d-none">
                <div class="d-flex justify-content-center my-3 title">
                  <h2 class="effect-typing-text-reg">Register</h2>
                </div>
                <form class="form-not-mg" id="form-register" method="POST">
                  <div class="form-group d-flex">
                    <div class="mr-2 w-100">
                      <label for="fname">First name</label>
                      <input
                        type="text"
                        name="fname"
                        class="form-control"
                        id="fnam"
                        aria-describedbsy="fnamelHelp"
                        placeholder="First name"
                      />
                    </div>
                    <div class="ml-2 w-100">
                      <label for="lname">Last name</label>
                      <input
                        type="text"
                        name="lname"
                        class="form-control"
                        id="lname"
                        aria-describedbsy="lnameHelp"
                        placeholder="Last name"
                      />
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-12">
                      <div class="form-group">
                        <label for="reg_email">Email</label>
                        <input
                          type="email"
                          name="email"
                          class="form-control"
                          id="reg_email"
                          placeholder="Email"
                        />
                        <small id="Help" class="form-text text-muted">
                          Ideally, you should register your email with paypal
                          email
                        </small>
                      </div>
                      <div class="form-group">
                        <label for="reg_password">Password</label>
                        <div class="input-group">
                          <input
                            type="password"
                            name="password"
                            class="form-control"
                            id="reg_password"
                            placeholder="Password"
                          />
                          <span
                            id="eye-pw_show"
                            class="input-group-text"
                            style="cursor: pointer"
                            data-visible="visible"
                            ><i class="las la-eye-slash la-2x"></i
                          ></span>
                          <span
                            id="eye-pw_hide"
                            class="input-group-text hide-pw"
                            style="cursor: pointer"
                            data-visible="hidden"
                            ><i class="las la-eye la-2x"></i
                          ></span>
                        </div>
                        <small
                          id="passwordHelp"
                          class="form-text text-muted mt-2"
                        >
                          Add 9 charachters or more, lowercase letters,
                          uppercase letters, numbers and symbols to make the
                          password really strong!
                        </small>
                      </div>
                      <div class="form-group">
                        <label for="re_password">Confirm Password</label>
                        <div class="input-group">
                          <input
                            type="password"
                            name="re_password"
                            class="form-control"
                            id="re_password"
                            placeholder="Re-Password"
                            type="password"
                          />
                          <span
                            style="cursor: pointer"
                            id="eye-pw-confirm_show"
                            class="input-group-text"
                            data-visible="visible"
                            ><i class="las la-eye-slash la-2x"></i
                          ></span>
                          <span
                            style="cursor: pointer"
                            id="eye-pw-confirm_hide"
                            data-visible="hidden"
                            class="input-group-text hide-pw"
                            ><i class="las la-eye la-2x"></i
                          ></span>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="form-group col-md-12">
                    <div
                      class="g-recaptcha"
                      data-sitekey="6Lexz1YUAAAAAJZknL3EkeY_xBlIKGKGfGwFHhjK"
                    ></div>
                  </div>
                  <div class="btn-area mb-1">
                    <button type="button" id="btn-reg" class="btn-register">
                      Register
                    </button>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
