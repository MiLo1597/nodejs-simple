<form action="./login" method="post" autocomplete="on">
        <div class="form-group">
            <label for="email">Email address</label>
            <input autocomplete="on" type="email" name="email" class="form-control" id="email" value="admin@noobteam.com" aria-describedby="emailHelp" placeholder="Enter email">
            <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
        </div>
        <div class="form-group">
            <label for="password">Password</label>
            <input autocomplete="on" type="password" name="password" class="form-control" id="password" value="123456" placeholder="Password">
        </div>
        <div class="form-group">
            <!-- <input type="checkbox" class="form-check-input" id="exampleCheck1"> -->
            <!-- <a href="./signup">Click here to signup</a> -->
            <!-- <label class="form-check-label" for="exampleCheck1">Check me out</label> -->
        </div>
        <button type="submit" id="btn-submit" class="btn btn-primary btn-login">Submit</button>
</form>